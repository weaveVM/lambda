{
  "name": "lambda",
  "version": "0.0.1",
  "description": "lazy evaluated calldata computation",
  "main": "index.node",
  "scripts": {
    "start": "node ./app/index.js",
    "test": "cargo test",
    "cargo-build": "cargo build --message-format=json > cargo.log",
